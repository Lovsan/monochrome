{
  "name": "monochrome-bot",
  "version": "2.3.9",
  "description": "A flexible Discord bot core based on Eris",
  "main": "monochrome.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "lintfix": "./node_modules/.bin/eslint . --fix",
    "test": "./node_modules/.bin/mocha",
    "coverage": "./node_modules/.bin/nyc npm test",
    "test_travis": "npm run lint && npm run coverage && ./node_modules/.bin/nyc report --reporter=text-lcov > coverage.lcov && ./node_modules/.bin/codecov",
    "document": "./node_modules/.bin/jsdoc --configure .jsdoc.json ./core/ -d ./docs -R README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mistval/monochrome.git"
  },
  "keywords": [
    "discord",
    "bot",
    "core",
    "framework",
    "library",
    "command",
    "commands",
    "setting",
    "settings"
  ],
  "author": "mistval",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mistval/monochrome/issues"
  },
  "homepage": "https://github.com/mistval/monochrome#readme",
  "dependencies": {
    "async-on-exit": "0.1.0",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "eris": "^0.10.1",
    "fpersist": "^1.0.1",
    "require-reload": "0.2.2",
    "time-stamp": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "docdash": "^1.0.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "jsdoc": "^3.6.3",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1"
  }
}
